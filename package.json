    "@prisma/client": "^6.13.0",
    "apollo-server-express": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-upload": "^17.0.0",
    "tesseract.js": "^6.0.1"
  },
